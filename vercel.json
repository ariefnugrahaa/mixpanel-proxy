{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/mixpanel.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/mixpanel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
